interface student
{
int score=25;
void displayscore();
}
interface sports
{
int sscore=25;
void displaysports();
}
class result implements student,sports
{
public void displayscore()
{
System.out.println("academicscore"+student.score);
}
public void displaysports()
{
System.out.println("sports score"+sports.sscore);
}
}
public class sportsresult
{
public static void main(String args[])
{
result r=new result();
r.displayscore();
r.displaysports();
}
}
